-- INSERT, UDAPTE, DELETE, SELECT
SELECT * FROM USUARIO;

SELECT NICKNAME, NOMBRE_APELLIDO FROM USUARIO
WHERE EMAIL like 'moises@gmail.com'
AND ROLL like 'admin'

SELECT LR.DESCRIPCION
FROM LISTA_REPRODUCCION LR JOIN USUARIO U
ON LR.NICKNAME = U.NICKNAME
WHERE U.EMIL like 'moises@gmail.com'

INSERT INTRO USUARIO
VALUES('moises a', 'moises@gmail.com', '12345', 'user');

INSERT INTO LISTA_REPRODUCCION
VALUES(deafult, 'Verano mix 2023', 'moises');